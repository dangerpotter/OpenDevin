{
    "name": "My Dev Environment with Docker, Python, and NodeJS",
    "image": "mcr.microsoft.com/vscode/devcontainers/universal:linux",
    "settings": {},
    "features": {
        "docker-in-docker": "latest"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "dbaeumer.vscode-eslint"
            ]
        }
